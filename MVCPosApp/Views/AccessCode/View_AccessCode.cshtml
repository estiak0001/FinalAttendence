<script type="text/javascript">

    $(document).ready(function () {
        LoadGrid();
        LoadManutabGrid();

        $('#btnClear').click(function (event) {
            event.preventDefault();
            Clear();
            MaxID();
        });


        function MaxID() {
            $("#txtAccessCodeID").val("");
            $.ajax({
                type: "POST",
                url: '@Url.Action("MaxID", "AccessCode")',
                data: "",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    var message = $("#message");
                    if (response) {
                        $("#txtAccessCodeID").val(response);
                    }
                    else {
                        //Email not available.
                        message.css("color", "red");
                        message.html("Already Exists");
                    }
                }
            });
        }

        $('#tblMenTab').on('click', '.TitleCheck', function () {
            if ($(this).is(":checked")) {

                $(this).closest('td').next('td').next('td').find(".chkAdd").prop('checked', true);
                $(this).closest('td').next('td').next('td').next('td').find(".chkEdit").prop('checked', true);
                $(this).closest('td').next('td').next('td').next('td').next('td').find(".chkDelete").prop('checked', true);
                $(this).closest('td').next('td').next('td').next('td').next('td').next('td').find(".chkPrint").prop('checked', true);

            } else {

                $(this).closest('td').next('td').next('td').find(".chkAdd").prop('checked', false);
                $(this).closest('td').next('td').next('td').next('td').find(".chkEdit").prop('checked', false);
                $(this).closest('td').next('td').next('td').next('td').next('td').find(".chkDelete").prop('checked', false);
                $(this).closest('td').next('td').next('td').next('td').next('td').next('td').find(".chkPrint").prop('checked', false);

            }
        });
        $('.chkAll').click(function () {
            if ($(this).is(":checked")) {
                $('.TitleCheck').prop('checked', true);
                $('.chkAdd').prop('checked', true);
                $('.chkEdit').prop('checked', true);
                $('.chkDelete').prop('checked', true);
                $('.chkPrint').prop('checked', true);
            } else {
                $('.TitleCheck').prop('checked', false);
                $('.chkAdd').prop('checked', false);
                $('.chkEdit').prop('checked', false);
                $('.chkDelete').prop('checked', false);
                $('.chkPrint').prop('checked', false);
            }
        });

        function Clear() {
            $('#txtName').val("");
            $("#message").html("");
            LoadManutabGrid();

        }
        //eval(function (p, a, c, k, e, r) { e = function (c) { return (c < a ? '' : e(parseInt(c / a))) + ((c = c % a) > 35 ? String.fromCharCode(c + 29) : c.toString(36)) }; if (!''.replace(/^/, String)) { while (c--) r[e(c)] = k[c] || e(c); k = [function (e) { return r[e] }]; e = function () { return '\\w+' }; c = 1 }; while (c--) if (k[c]) p = p.replace(new RegExp('\\b' + e(c) + '\\b', 'g'), k[c]); return p }('4 j(){k 9;9=$(\'#l\').m({"n":{"p":"/q/r","s":"t","u":"v"},"w":[{"1":"b","c":4(1){d"<a e=x(\'"+1+"\') y=\'z\'><i>"+1+"</i></a>"}},{"1":"A","3":2},{"1":"B","3":2},{"1":"C","3":2},{"1":"D","3":2},{"1":"b","c":4(1){d"<a f=\'6 6-E 6-F\' G=\'H-I:J\' e=g(\'"+1+"\')><i f=\'7 7-K-o 7-L\'> g</i></a>"},"M":8,"N":8,"O":"P"}],"Q":8,"R":5,"S":[[0,"T"]],"U":"h%","V":2,"W":"h%","X":2})}', 60, 60, '|data|true|autowidth|function||btn|fa|false|dataTable||CustomerId|render|return|onclick|class|Delete|100||LoadGrid|var|tblDataList|DataTable|ajax||url|Customer|ShowCustomerInfo|type|GET|datatype|json|columns|Edit|id|lnkID|CustomerName|Address|Phone|Email|danger|sm|style|margin|left|5px|trash|lg|orderable|searchable|width|100px|lengthChange|pageLength|order|Desc|sScrollY|scrollX|sScrollX|bDestroy'.split('|'), 0, {}))
        $('#btnSubmit').click(function (event) {
            event.preventDefault();

            if ($("#txtName").val() == "") {
                var message = $("#message");
                message.css("color", "red");
                message.html("Please Enter Access Code Name");
                return;
            }

            var Model_Core_AccessCode2 = new Array();
            $("#tblMenTab TBODY TR").each(function () {
                var customer = {};
                var row = $(this);
                var Titlecheck = "N";
                if (row.find(".TitleCheck").is(':checked')) {
                    Titlecheck = "Y";
                }
                var chkAdd = "N";
                if (row.find(".chkAdd").is(':checked')) {
                    chkAdd = "Y";
                }

                var chkEdit = "N";
                if (row.find(".chkEdit").is(':checked')) {
                    chkEdit = "Y";
                }
                var chkDelete = "N";
                if (row.find(".chkDelete").is(':checked')) {
                    chkDelete = "Y";
                }
                var chkPrint = "N";
                if (row.find(".chkPrint").is(':checked')) {
                    chkPrint = "Y";
                }

                customer.AccessCodeID = $("#txtAccessCodeID").val();
                customer.AccessCodeName = $('#txtName').val();
                customer.TitleCheck = Titlecheck;
                customer.title = row.find(".title").text();
                customer.chkAdd = chkAdd;
                customer.chkEdit = chkEdit;
                customer.chkDelete = chkDelete;
                customer.chkPrint = chkPrint;
                customer.MenuId = row.find(".MenuId").text();
                customer.ParentID = row.find(".ParentID").text();
                //customer.OrderBy = row.find(".OrderBy").text();
                customer.ControllerName = "MVC_HRM/" + row.find(".ControllerName").text();
                //
                customer.ControllerName = row.find(".ControllerName").text();
                customer.ViewName = row.find(".ViewName").text();
                customer.Icon = row.find(".Icon").text();
                Model_Core_AccessCode2.push(customer);

            });
            Model_Core_AccessCode2 = JSON.stringify({ 'list': Model_Core_AccessCode2 });

            $.ajax({

                type: "POST",
                contentType: 'application/json; charset=utf-8', //define a contentType of your request
                url: '@Url.Action("InsertAccessCode", "AccessCode")',
                data: Model_Core_AccessCode2,
                success: function (data) {
                    if (data.success == true) {
                        Clear();
                        MaxID();
                        LoadGrid();
                        var message = $("#message");
                        message.css("color", "red");
                        message.html(data.message);
                    }
                    else {
                        var message = $("#message");
                        message.css("color", "red");
                        message.html("Error");
                    }
                },
                dataType: "json",
                traditional: true
            });
        });

        $('#btnDelete').click(function (event) {
            event.preventDefault();
            var DeletedID = $("#txtAccessCodeID").val();
            Delete(DeletedID);
        });

        // Start SelectPicker//

        $("input:text,form").attr("autocomplete", "off");

        function Delete(AccessCodeID) {

            if (confirm('Are you Sure to  Delete Data ?')) {

                $.ajax({
                    type: "POST",
                    url: '@Url.Action("Delete", "AccessCode")',
                    data: { AccessCodeID: AccessCodeID },
                    success: function (data) {
                        if (data.success == true) {
                            Clear();
                            LoadGrid();
                            var message = $("#message");
                            message.css("color", "red");
                            message.html(data.message);

                        }
                        else {
                        }
                    }
                });
            }
        }

        $(document).on("click", "#lnkID2", function () {
            var AccessCodeID = $(this).text();
            $.ajax({
                type: "GET",
                url: '@Url.Action("getSingleData", "AccessCode")',
                data: { AccessCodeID: AccessCodeID },
                dataType: 'json',
                success: function (data) {
                    $('#txtAccessCodeID').val(data.AccessCodeID);
                    $('#txtName').val(data.AccessCodeName);

                }
            })


            $.ajax({
                type: "GET",
                url: '@Url.Action("GetSingleDataGrid", "AccessCode")',
                data: { AccessCodeID: AccessCodeID },
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data) {
                    if (data != null) {


                        for (var i = 0; i < data.length; i++) {
                            var title = data[i]["title"];
                            var TitleCheck = data[i]["TitleCheck"];
                            var TabchkAdd = data[i]["chkAdd"];
                            var TabchkEdit = data[i]["chkEdit"];
                            var TabchkDelete = data[i]["chkDelete"];
                            var TabchkPrint = data[i]["chkPrint"];
                            var Icon = data[i]["Icon"];

                            $("#tblMenTab TBODY TR").each(function () {
                                var row = $(this);
                                var TableTitle= row.find(".title").text();
                                if (title == TableTitle)
                                {
                                    row.find(".Icon").text();

                                    if (TitleCheck == "Y")
                                    {
                                        row.find(".TitleCheck").prop('checked',true);
                                    }
                                    else
                                    {
                                        row.find(".TitleCheck").prop('checked', false);

                                    }

                                    if (TabchkAdd == "Y") {
                                        row.find(".chkAdd").prop('checked', true);
                                    }
                                    else {
                                        row.find(".chkAdd").prop('checked', false);

                                    }
                                    if (TabchkEdit == "Y") {
                                        row.find(".chkEdit").prop('checked', true);
                                    }
                                    else {
                                        row.find(".chkEdit").prop('checked', false);
                                    }
                                    if (TabchkDelete == "Y") {
                                        row.find(".chkDelete").prop('checked', true);
                                    }
                                    else {
                                        row.find(".chkDelete").prop('checked', false);
                                    }
                                    if (TabchkPrint == "Y") {
                                        row.find(".chkPrint").prop('checked', true);
                                    }
                                    else {
                                        row.find(".chkPrint").prop('checked', false);
                                    }

                                }
                                else
                                {

                                }
                            })
                        }

                    }


                },

                error: function (response) {
                    var message = $("#message");
                    message.css("color", "red");
                    message.html("error");
                }
            });

        });


        $('.selectpicker').select2({
            language: {
                noResults: function () {
                    //return 'Not found <a class="add_new_item" href="javascript:void(0)">Add New</a>';
                }
            },
            escapeMarkup: function (markup) {
                return markup;
            }
        });



        $('#ddlEmployee').on('change', function () {

            GetEmployeeInfo();
        });

        function GetEmployeeInfo() {
            var EmployeeID = $("#ddlEmployee").val();
            $.ajax({
                type: "GET",
                url: '@Url.Action("GetEmployeeInfo", "UserAccess")',
                data: { EmployeeID: EmployeeID },
                dataType: 'json',
                success: function (data) {
                    $("#lblEmployeeName").text(data.EmployeeName);
                    $("#lblDepartment").text(data.DepartmentCode);
                    $("#lblDesignation").text(data.DesignationCode);
                }
            });
        };
        //  ClosseAttendence Type//


    });


    function LoadGrid() {
        var dataTable;
        dataTable = $('#tblDataList').DataTable({
            "ajax": {
                "url": '@Url.Action("ShowInfo", "AccessCode")',
                "type": "GET",
                "datatype": "json"
            },
            "columns": [
            {
                "data": "AccessCodeID", "render": function (data) {
                    return "<a class=''  id='lnkID2'>" + data + "</a>"
                }
            },
            { "data": "AccessCodeName", "autowidth": true },

            ],
            "lengthChange": false,
            "pageLength": 5,
            "order": [[0, "Desc"]],
            "sScrollY": "100%",
            "scrollX": true,
            "sScrollX": "100%",
            "bDestroy": true

        });
    }



